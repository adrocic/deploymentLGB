{
    "name": "lolgamebuddy-backend",
    "version": "1.0.0",
    "description": "An app",
    "main": "server.ts",
    "type": "module",
    "scripts": {
        "start": "node -r ts-node/register server.ts",
        "start:watch": "nodemon",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "heroku-postbuild": "cd Client && npm install && npm run build"
    },
    "author": "Anel Drocic",
    "license": "ISC",
    "dependencies": {
        "@types/express": "^4.17.2",
        "@types/mongoose": "^5.7.0",
        "@types/node": "^13.7.0",
        "axios": "^0.19.2",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "mongoose": "^5.8.11",
        "node": "^12.14.1",
        "nodemon": "^2.0.2",
        "ts-node": "^8.6.2",
        "typescript": "^3.7.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsc --noEmit && lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "devDependencies": {
        "@types/cors": "^2.8.6",
        "@types/react-router-dom": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^2.16.0",
        "@typescript-eslint/parser": "^2.16.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.0.9",
        "lint-staged": "^10.0.0-beta.15",
        "prettier": "^1.19.1"
    }
}
